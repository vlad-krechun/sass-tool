{
	// Place your snippets for scss here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Hover only without touchscreen": {
		"prefix": "mhov",
		"body": [
		  "@media(hover: hover) and (pointer: fine) {",
		  "  &:hover{",
		  "    $1",
		  "  }",
		  "}"
		],
		"description": "Hover only without touchscreen"
	  },
	  "Media min width 576": {
		"prefix": "@mism",
		"body": [
		  "@media (min-width: 576px) {",
		  "  $1",
		  "}"
		],
		"description": "Media min width 576"
	  },

	  "Media min width 768": {
		"prefix": "@mimd",
		"body": [
		  "@media (min-width: 768px) {",
		  "    $1",
		  "}"
		],
		"description": "Media min width 768"
	  },
	  "Media min width 992": {
		"prefix": "@milg",
		"body": [
		  "@media (min-width: 992px) {",
		  "    $1",
		  "}"
		],
		"description": "Media min width 992"
	  },
	  "Media min width 1200": {
		"prefix": "@mixlg",
		"body": [
		  "@media (min-width: 1200px) {",
		  "    $1",
		  "}"
		],
		"description": "Media min width 1200"
	  },
	  "Media min width 1400": {
		"prefix": "@mixxlg",
		"body": [
		  "@media (min-width: 1400px) {",
		  "    $1",
		  "}"
		],
		"description": "Media min width 1400"
	  },
	  "Media max width 576 - 0.2": {
		"prefix": "@maxs",
		"body": [
		  "@media (max-width: 575.98px) {",
		  "  $1",
		  "}"
		],
		"description": "Media max width 576 - 0.2"
	  },
	  "Media max width 768 - 0.2": {
		"prefix": "@masm",
		"body": [
		  "@media (max-width: 767.98px) {",
		  "  $1",
		  "}"
		],
		"description": "Media max width 768 - 0.2"
	  },
	  "Media max width 992 - 0.2": {
		"prefix": "@mamd",
		"body": [
		  "@media (max-width: 991.98px) {",
		  "  $1",
		  "}"
		],
		"description": "Media max width 992 - 0.2"
	  },
	  "Media max width 1200 - 0.2": {
		"prefix": "@malg",
		"body": [
		  "@media (max-width: 1199.98px) {",
		  "  $1",
		  "}"
		],
		"description": "Media max width 1200 - 0.2"
	  },
	  "Media max width 1400 - 0.2": {
		"prefix": "@maxlg",
		"body": [
		  "@media (max-width: 1399.98px) {",
		  "  $1",
		  "}"
		],
		"description": "Media max width 1400 - 0.2"
	  },
	  "Mixin size fluid": {
		"prefix": "mix-fluid",
		"body": [
		  "@include mixFluid(${1:property}, ${2:min-size}, ${3:max-size}, ${4:320}, ${5:1400});"
		],
		"description": "Mixin size fluid"
	  },
	  "Function lighter color": {
		"prefix": "f-lig",
		"body": [
		  " lighter(${1:color}, ${2:tone});"
		],
		"description": "Function lighter color"
	  },
	  "Function darker color": {
		"prefix": "f-dar",
		"body": [
		  " darker(${1:color}, ${2:tone});"
		],
		"description": "Function darker color"
	  }


	 




